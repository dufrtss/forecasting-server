// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String   @id @default(uuid())
  name       String
  company    String
  number     String
  email      String   @unique
  password   String
  created_at DateTime @default(now())

  @@map("users")
}

model Flow {
  id        Int   @id @default(autoincrement())
  value     Float
  timestamp Float

  @@map("flow")
}

model Forecasting {
  id        Int   @id @default(autoincrement())
  value     Float
  timestamp Float

  @@map("forecasting")
}
